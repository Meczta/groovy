pipeline {
agent any
options {

    buildDiscarder(logRotator(numToKeepStr: '5'))
    disableConcurrentBuilds()
    timeout(time: 10, unit: 'MINUTES')
}
triggers {
    pollSCM('* * * * *')
}
stages {
    stage('Example') {
            steps {
                echo 'Hello World'
            }
        }
    stage("First step") {
        steps {
            sh 'ssh dream@192.168.56.102 \'docker run https://github.com/meczta/sayt.git\''
            }
        }
    }  
}
